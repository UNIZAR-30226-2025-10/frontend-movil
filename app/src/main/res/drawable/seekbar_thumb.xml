<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Fondo izquierdo debajo del círculo: azul -->
    <item
        android:left="0dp"
        android:right="5dp"
        android:top="0dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="@color/blueNuestro" />
        </shape>
    </item>

    <!-- Fondo izquierdo debajo del círculo: azul -->
    <item
        android:left="5dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="0dp">
        <shape>
            <solid android:color="@color/seekbar" />
        </shape>
    </item>

    <item
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp">
        <shape android:shape="oval">
            <size android:width="10dp" android:height="10dp" />
            <solid android:color="@color/blueNuestro" />
        </shape>
    </item>
</layer-list>
